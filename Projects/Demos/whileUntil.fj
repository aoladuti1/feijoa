sysln!("This program prints numbers 1 to 10 in ascending order,
and then in descending order, using 'while' and 'until' loops\n");

x = 0;

until (x == 10) {
	x += 1;
	sysln!(x);
}

sysln!("_______________");

while (x > 0) {
	sysln!(x);
	x -= 1;
}

// continue -- keyword works same as python/java
// break --- keyword works same as python/java