struct X {
	a = "1"; // to show that structs can hold different types

	struct Y {
		b = 6;
		// err = new X(); --- returns an error - you can't construct the containing struct
		def c() { sysout!("max"); } // c is added as a member variable
	}
}

a = new X.Y(7).b; // accessing r-values is possible, alongside nested structs
b = new X.Y(7, c=8); // see functions = first class variables, and arg selection
callC = new X.Y();
callC.c(); // prints "max"
