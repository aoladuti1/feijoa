// exponentiation
x <- 7;
sysout("The following prints 49: ");
sysline(x ** 2); // 49

sysout("The following prints 5: ");
sysline(25 ** 0.5);


// minus 
x <- 10;
sysout("The following also prints 5: ");
sysline(15 - x);

lie <- "i don't like computer science. i really don't like it.";
truth_sometimes <- lie - " don't";
sysout("The following is prints a line about me liking comp sci: ");
sysline(truth_sometimes);


// not
sysout("Prints true: ");
sysline(!false);

sysout("Prints false: ");
sysline(!(x > 2));

// gives error: sysline(!x > 2); (which is what we want)


// and
sysout("Prints true: ");
sysline(!((x > 2) && (x > 100)));

sysout("Prints false: ");
sysline(!((x > 2) && (x > 1)));


// boolean algebra
T <- true;
F <- false;
sysout("Prints true: ");
sysline(T * F + T);

