x = 0;
sysln!("> x = 0");
sysout!("> sysln!(x++)"); sysln!(" outputs: " + x++); // postfix incr. (outputs 0)
sysout!("> sysln!(x)"); sysln!(" outputs: " + x); // outputs 1

sysln!("-----------------");

y = 0;
sysln!("> y = 0");
sysout!("> sysln!(++y)"); sysln!(" outputs: " + ++y); // prefix incr. // outputs 1

sysln!("-----------------");

sysln!("Equivalent decrement operations exist (e.g. x-- or --x)");